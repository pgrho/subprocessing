<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <sharedListeners>
      <add name="console"
           type="System.Diagnostics.ConsoleTraceListener" />
    </sharedListeners>
    <sources>
      <source name="Shipwreck.Subprocessing.Subprocess"
              switchType="System.Diagnostics.SourceSwitch"
              switchValue="All" >
        <listeners>
          <add name="console" />
        </listeners>
      </source>
    </sources>
  </system.diagnostics>
</configuration>